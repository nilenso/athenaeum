{
  "name": "athenaeum",
  "scripts": {
    "start": "shadow-cljs watch app",
    "clean": "rm -rf resources/public/css/ && rm -rf resources/public/js/",
    "release": "yarn clean && shadow-cljs release app --config-merge '{:closure-defines {athenaeum-web.core/client-id \"$STAGING_CLIENT_ID\"}}'",
    "test": "shadow-cljs compile test && node target/test.js"
  },
  "devDependencies": {
    "shadow-cljs": "^2.11.8"
  },
  "dependencies": {
    "highlight.js": "9.18.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-highlight.js": "1.0.7",
    "xmlhttprequest": "^1.8.0"
  }
}
